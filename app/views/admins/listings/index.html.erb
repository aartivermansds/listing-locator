<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Banner image</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Zipcode</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Contact</th>
      <th>Email</th>
      <th>Website</th>
      <th>Address</th>
      <th>Fb url</th>
      <th>Linkedin url</th>
      <th>Gplus url</th>
      <th>Twitter url</th>
      <th>Listing category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
    <tr>
      <td><%= listing.name %></td>
      <td><%= listing.description %></td>
      <td><%= listing.banner_image %></td>
      <td><%= listing.city %></td>
      <td><%= listing.state %></td>
      <td><%= listing.country %></td>
      <td><%= listing.zipcode %></td>
      <td><%= listing.latitude %></td>
      <td><%= listing.longitude %></td>
      <td><%= listing.contact %></td>
      <td><%= listing.email %></td>
      <td><%= listing.website %></td>
      <td><%= listing.address %></td>
      <td><%= listing.fb_url %></td>
      <td><%= listing.linkedin_url %></td>
      <td><%= listing.gplus_url %></td>
      <td><%= listing.twitter_url %></td>
      <td><%= listing.listing_category %></td>
      <td><%= link_to 'Show', admins_listing_path(listing) %></td>
      <td><%= link_to 'Edit', edit_admins_listing_path(listing) %></td>
      <td><%= link_to 'Destroy', admins_listing_path(listing), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_admins_listing_path %>
